{
  "trailingComma": "none",
  "useTabs": true,
  "tabWidth": 4,
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "THIRD_PARTY_MODULES",
    "^@/components/(.*)$",
    "^@/providers/(.*)$",
    "^@/services/(.*)$",
    "^@/assets/(.*)$",
    "^@/types/(.*)$",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "^(.scss)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
